{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "mgmt-lib": [
        "dist/mgmt-lib"
      ],
      "mgmt-lib/*": [
        "dist/mgmt-lib/*"
      ],
      "@app/*": ["projects/management/src/app/*"]
    }
  }
}
